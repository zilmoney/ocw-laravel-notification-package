{
    "name": "laravel-notification-channels/onlinecheckwriter",
    "description": "OnlineCheckWriter notifications channel for Laravel",
    "keywords": [
        "laravel",
        "notifications",
        "onlinecheckwriter",
        "mail",
        "checks",
        "postcards"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zilmoney",
            "email": "arjun.uk@zilmoney.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Zilmoney\\OnlineCheckWriter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zilmoney\\OnlineCheckWriter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zilmoney\\OnlineCheckWriter\\OnlineCheckWriterServiceProvider"
            ]
        }
    }
}
