{
    "name": "laravel-notification-channels/onlinecheckwriter",
    "description": "OnlineCheckWriter notifications channel for Laravel - Send physical mail, checks, and documents",
    "keywords": [
        "laravel",
        "notifications",
        "onlinecheckwriter",
        "mail",
        "checks",
        "physical-mail",
        "documents"
    ],
    "homepage": "https://github.com/laravel-notification-channels/onlinecheckwriter",
    "license": "MIT",
    "authors": [
        {
            "name": "Zilmoney",
            "email": "developer@zilmoney.com",
            "homepage": "https://zilmoney.com"
        }
    ],
    "support": {
        "issues": "https://github.com/laravel-notification-channels/onlinecheckwriter/issues",
        "source": "https://github.com/laravel-notification-channels/onlinecheckwriter"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Zilmoney\\OnlineCheckWriter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zilmoney\\OnlineCheckWriter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zilmoney\\OnlineCheckWriter\\OnlineCheckWriterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
